<?xml version="1.0" encoding="UTF-8"?>
<configuration status="OFF">
<appenders>
 <Console name="Console" target="SYSTEM_OUT">
 <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%t] %-5level %30c{1.} - %msg%n" />
 </Console>
 </appenders>
 <Loggers>
 <Root level="info">
 <AppenderRef ref="Console" />
 </Root>
 </Loggers>
</configuration>